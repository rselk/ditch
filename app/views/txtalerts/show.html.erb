<%- model_class = Txtalert -%>
<div class="page-header">
  <h1>Alert Recorded</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:time_alert) %>:</strong></dt>
  <dd><%= @txtalert.time_alert %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contents) %>:</strong></dt>
  <dd><%= @txtalert.contents %></dd>
  <dt><strong><%= model_class.human_attribute_name(:to_sms) %>:</strong></dt>
  <dd><%= @txtalert.to_sms %></dd>

</dl>

<div class="form-actions">
<%= link_to 'Back', manage_path, :class => 'btn btn-success' %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_txtalert_path(@txtalert), :class => 'btn btn-warning' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              txtalert_path(@txtalert),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
